<?xml version="1.0" encoding="utf-8" ?>

<!--
 - this is just template file of your actual usercredentials.xml file
 - Where your supposed to store your important credentials and configurations
 - according to following xml structure
 -
 -->

<resources>
    <api-keys>
        <key name="api name" value="your api-key" />
        <key name="firebase API-key" value="your firebase API-key" />
        <key name="remote sql API-key" value="API-key given to you" />
        <!--
            List of all your API key
        -->
    </api-keys>
    <credentials>
        <auth name="service name for credentials" username="username for that service" password="passcode for that service" />
        <auth name="remote database" username="database usernme" password="database password" />
        <!--
        - List of all your Authentication Credentials for varius purposes
        -->
    </credentials>

    <urls>
        <url name="url name" value="url" />
        <url name="sql connection url" value="jdbc:postgresql://host:port/database" />
        <!--
          - List of URLs
          -->
    </urls>
</resources>